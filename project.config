[project]
	description = GitHub Mylyn integration (technology.egit)
[receive]
	requireChangeId = true
[access]
	inheritFrom = permissions/egit-dev
[plugin "uploadvalidator"]
	blockedContentTypeWhitelist = false
	invalidFilenamePattern = ^tools/jenkins/.*
	maxPathLength = 0
	rejectDuplicatePathnames = false
	rejectDuplicatePathnamesLocale = en
	rejectSubmodule = false
	rejectSymlink = false
	rejectWindowsLineEndings = false
	skipGroup = ldap/technology.egit
	skipValidation = invalidFilenamePattern